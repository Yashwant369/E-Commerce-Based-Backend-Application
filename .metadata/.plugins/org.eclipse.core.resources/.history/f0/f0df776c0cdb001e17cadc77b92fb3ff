package com.yashwant.user_service.entity;

import lombok.Data;

@Data
public class Cart {
	
	
	private String cartId;	
	private Date createdDate;
	@OneToOne
	private User user;
	@OneToMany(mappedBy = "cart", cascade = CascadeType.ALL,orphanRemoval = true)
	private List<CartItem>cartItems = new ArrayList<>();

}
